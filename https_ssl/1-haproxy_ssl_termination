defaults
        mode    http
        option  httplog
        option  dontlognull
        timeout connect 5000
        timeout client  50000
        timeout server  50000

frontend http-in
        bind *:80
        bind *:443 ssl crt /etc/letsencrypt/live/www.raisa.tech/fullchain.pem
        mode http
        default_backend webservers

backend webservers
        balance roundrobin
        server web-01 54.162.217.247:80 check
        server web-02 35.172.214.12:80 check


frontend https_frontend
        bind *:80
        bind *:443 ssl crt /etc/letsencrypt/live/www.raisa.tech/fullchain.pem
        mode http
        default_backend webservers
